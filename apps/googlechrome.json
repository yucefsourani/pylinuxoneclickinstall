{
    "__arch__"           : ["x86_64"],
    "__distro__"         : ["fedora"],
    "__distro_version__" : ["all"],
    "__desktop__"        : ["all"],
    "__commands__"       : ["echo -e '[google-chrome]\nname=google-chrome\nbaseurl=http://dl.google.com/linux/chrome/rpm/stable/x86_64\nenabled=1\ngpgcheck=1\ngpgkey=https://dl.google.com/linux/linux_signing_key.pub' |tee pkexec /etc/yum.repos.d/google-chrome.repo",
            "pkexec dnf config-manager --set-enabled google-chrome",
            "pkexec dnf install google-chrome-stable -y --best"]

}
